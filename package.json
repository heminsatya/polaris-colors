{
  "name": "polaris-colors",
  "version": "1.4.1",
  "description": "Polaris Colors Plugin for Polaris Framework. A set of beautiful flat colors that you can use in your web apps.",
  "author": "Hemin Satya",
  "license": "MIT",
  "main": "index.js",
  "style": "/dist/css/",
  "sass": "/src/scss/",
  "keywords": [
    "CSS",
    "Sass",
    "JavaScript",
    "TypeScript",
    "webpack",
    "HTML",
    "responsive-design",
    "front-end",
    "Web",
    "Framework"
  ],
  "homepage": "https://github.com/heminsatya/polaris-colors#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heminsatya/polaris-colors.git"
  },
  "bugs": {
    "url": "https://github.com/heminsatya/polaris-colors/issues"
  },
  "devDependencies": {
    "polaris-core": "^0.9.12",
    "sass": "^1.56.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "main": "sass src/scss/main.scss dist/css/main.min.css --style compressed --load-path=node_modules",
    "main-dev": "sass src/scss/main.scss dist/css/main.css --watch --load-path=node_modules",
    "polaris": "sass src/scss/polaris.scss dist/css/polaris.min.css --style compressed --load-path=node_modules",
    "polaris-dev": "sass src/scss/polaris.scss dist/css/polaris.css --watch --load-path=node_modules",
    "material": "sass src/scss/material.scss dist/css/material.min.css --style compressed --load-path=node_modules",
    "material-dev": "sass src/scss/material.scss dist/css/material.css --watch --load-path=node_modules",
    "html": "sass src/scss/html.scss dist/css/html.min.css --style compressed --load-path=node_modules",
    "html-dev": "sass src/scss/html.scss dist/css/html.css --watch --load-path=node_modules",
    "flat": "sass src/scss/flat.scss dist/css/flat.min.css --style compressed --load-path=node_modules",
    "flat-dev": "sass src/scss/flat.scss dist/css/flat.css --watch --load-path=node_modules",
    "metro": "sass src/scss/metro.scss dist/css/metro.min.css --style compressed --load-path=node_modules",
    "metro-dev": "sass src/scss/metro.scss dist/css/metro.css --watch --load-path=node_modules",
    "social": "sass src/scss/social.scss dist/css/social.min.css --style compressed --load-path=node_modules",
    "social-dev": "sass src/scss/social.scss dist/css/social.css --watch --load-path=node_modules"
  }
}
